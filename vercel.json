{
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "functions": {
    "api/hubspot-forms.js": {
      "maxDuration": 10
    }
  },
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Frame-Options",
          "value": "ALLOWALL"
        }
      ]
    }
  ]
}